{
  "name": "@discord-bot/dnd5e",
  "version": "1.0.0",
  "description": "D&D 5e rules engine, data, and adapters",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./rules": {
      "import": "./dist/rules/index.js",
      "types": "./dist/rules/index.d.ts"
    },
    "./data": {
      "import": "./dist/data/index.js",
      "types": "./dist/data/index.d.ts"
    },
    "./adapters": {
      "import": "./dist/adapters/index.js",
      "types": "./dist/adapters/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@discord-bot/dnd5e-types": "*"
  },
  "devDependencies": {
    "@discord-bot/tsconfig": "*",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
